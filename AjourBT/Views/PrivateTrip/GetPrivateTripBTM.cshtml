@model string

@{
    Layout = null;
}

@{
    AjaxOptions ajaxOpts = new AjaxOptions
    {
        UpdateTargetId = "tableBodyPrivateTrip",
        Url = Url.Action("GetPrivateTripDataBTM")
    };
}


@using (Ajax.BeginForm(ajaxOpts))
{
    <div>
        <input id="seachInputVU"  type="search" name="searchString" value="@ViewBag.SearchString" placeholder="Search... " />
    </div>
}


<div id="tableBodyPrivateTrip">
    @Html.Action("GetPrivateTripDataBTM", new { searchString = ViewBag.SearchString })
</div>

